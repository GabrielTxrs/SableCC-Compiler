Package calculadora;

Helpers
 maiuscula = ['A' .. 'Z'];
 minuscula = ['a' .. 'z'];
 digito = ['0' .. '9'];
 espaco = ' ';
 rc = 13;
 nl = 10;
 
Tokens
 numero = digito+;
 identificador = maiuscula ( digito | maiuscula | minuscula |  '_' )*;
 soma = '+';
 menos = '-';
 mult = '*';
 div = '/';
 mod = '%';
 par_esq = '(';
 par_dir = ')';
 vazio = (espaco | rc | nl)+;


Ignored Tokens
 vazio;

